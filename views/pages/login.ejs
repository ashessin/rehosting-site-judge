<!DOCTYPE html>
<html lang="en">
<% include ../partials/head.ejs %>

<body>
<% include ../partials/nav.ejs %>
<main>
    <h1><%= title %></h1>
    <hr>
    <% include ../partials/url.ejs %>
    <form action="/home" method="post">
        <div class="row">
            <div class="col-4">
                <label for="username-rsj"></label>
                <input id="username-rsj"
                       name="username"
                       type="text"
                       placeholder="Username"
                       autocomplete="username">
                <label for="password-rsj"></label>
                <input id="password-rsj"
                       name="password"
                       type="password"
                       placeholder="Password"
                       autocomplete="current-password">
            </div>
            <div class="col">
                <button id="login-submit-rsj"
                        name="submit"
                        type="submit">
                    Submit
                </button>
            </div>
        </div>
    </form>
</main>
<script>
</script>
</body>

</html>
